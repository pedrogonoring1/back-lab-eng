{
  "name": "back-lab-eng",
  "version": "0.0.1",
  "scripts": {
    "lint": "eslint ./src/**/*.ts ./test/**/*.ts --max-warnings=0",
    "lint:fix": "eslint ./src/**/*.ts ./test/**/*.ts --fix",
    "clean": "rimraf dist",
    "compile": "tsc -p .",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --check",
    "prettier:format": "prettier --config .prettierrc 'src/**/*.ts' 'test/**/*.ts' --write",
    "format": "npm run lint:fix && npm run prettier:format",
    "start:dev": "tsnd --respawn src/server.ts",
    "build": "npm run clean && npm run compile",
    "start": "node dist/server.js",
    "test": "cross-env NODE_ENV=testing mocha --require ts-node/register --ui bdd 'test/**/*.test.ts'",
    "test:watch": "cross-env NODE_ENV=testing mocha --watch --require ts-node/register --ui bdd 'test/**/*.test.ts'"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^5.0.2",
    "inversify": "^6.0.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.5.2",
    "prompt-sync": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "sinon": "^14.0.0",
    "uuid": "^8.3.2",
    "winston": "~3.8.1",
    "yargs": "~17.5.1"
  },
  "devDependencies": {
    "@types/auth0": "^2.34.11",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.5",
    "@types/chance": "~1.1.3",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.0",
    "@types/mongoose": "^5.11.97",
    "@types/prompt-sync": "^4.1.1",
    "@types/sinon": "^10.0.11",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@types/yargs": "~17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chance": "~1.1.8",
    "cross-env": "^7.0.3",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^10.0.0",
    "mongodb-memory-server": "^8.6.1",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.5.5"
  }
}
